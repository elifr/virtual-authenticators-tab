{
  "name": "Virtual Authenticators Tab",
  "version": "0.9",
  "description": "A devtools tab to manage virtual authenticators",
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "permissions": [
    "debugger","tabs", "<all_urls>"
  ],

  "devtools_page": "devtools.html"
}
